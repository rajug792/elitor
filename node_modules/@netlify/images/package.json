{
  "name": "@netlify/images",
  "version": "1.2.2",
  "description": "TypeScript implementation of Netlify's Image CDN",
  "type": "module",
  "engines": {
    "node": ">=20.6.1"
  },
  "main": "./dist/main.js",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup-node",
    "prepack": "npm run build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "npm run build && vitest run",
    "dev": "tsup-node --watch",
    "publint": "npx -y publint --strict"
  },
  "keywords": [],
  "license": "MIT",
  "repository": "netlify/primitives",
  "bugs": {
    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@netlify/dev-utils": "^4.0.0",
    "tsup": "^8.5.0",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "ipx": "^3.0.3"
  }
}
